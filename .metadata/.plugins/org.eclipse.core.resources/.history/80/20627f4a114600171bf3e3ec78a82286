package practice;

import java.net.URL;
import java.util.Scanner;

public class GetUrl {

	public static void main(String[] args) throws Exception {
		URL u;
		Scanner s;
		u = new URL("http://www.vnrvjiet.ac.in");
		s = new Scanner(u.openStream());
		while(s.hasNext()){
			System.out.println(s.nextLine());
			
		}
	}

}
